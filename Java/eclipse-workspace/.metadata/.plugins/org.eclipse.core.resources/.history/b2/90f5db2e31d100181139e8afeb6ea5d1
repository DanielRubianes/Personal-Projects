public class danielRubianesMod4CircleTesterUpdated
{
    public static void main(String args[])
    {
        danielRubianesMod4CircleUpdated circle1 = new danielRubianesMod4CircleUpdated(5.1);
        danielRubianesMod4CircleUpdated circle2 = new danielRubianesMod4CircleUpdated(20.6);
        circle1.radius = 20;
        System.out.println(circle1.circumference());
    }
}

/*
 * Sample Output: 
 * 125.66370614359172
